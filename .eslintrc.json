{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["flowtype", "prettier", "jest"],
  "globals": {
    "stream$Readable": true,
    "tty$ReadStream": true,
    "stream$Writable": true,
    "tty$WriteStream": true
  },
  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "prettier/prettier": 2
  },
  "env": {
    "jest/globals": true
  }
}
